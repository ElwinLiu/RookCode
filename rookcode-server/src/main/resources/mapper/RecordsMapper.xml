<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.server.mapper.RecordsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.server.pojo.Records">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="question_id" property="questionId" />
        <result column="lang_id" property="langId" />
        <result column="result" property="result" />
        <result column="exec_time" property="execTime" />
        <result column="memory" property="memory" />
        <result column="submit_date" property="submitDate" />
        <result column="log" property="log" />
        <result column="state" property="state" />

        <result column="test_case_total_num" property="testCaseTotalNum" />
        <result column="test_case_access_num" property="testCaseAccessNum" />
        <result column="submit_code" property="submitCode" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, question_id, lang_id, result, exec_time, memory, submit_date, log, state, test_case_total_num,test_case_access_num,submit_code
    </sql>

</mapper>
